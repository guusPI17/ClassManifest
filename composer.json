{
    "name": "guuspi17/class-manifest",
    "description": "Class manifest. Retrieves all classes that implement the specified interface.",
    "type": "library",
    "keywords": [
        "php",
        "class",
        "manifest",
        "extends",
        "implements"
    ],
    "license": "MIT",
    "homepage": "https://github.com/guusPI17/ClassManifest",
    "authors": [
        {
            "name": "Gusev Anton",
            "email": "antongusevpi17@yandex.com"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "vendor/bin/phpunit --colors=always",
        "cs": "phpcs --standard=ruleset.xml --encoding=utf-8 -s"
    },
    "require": {
        "php": ">=7.4",
        "nikic/php-parser": "^5.1",
        "psr/simple-cache": "^1.0"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "^3.5",
        "phpunit/phpunit": "9.0"
    },
    "autoload": {
        "psr-4": {
            "Guuspi17\\ClassManifest\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Guuspi17\\ClassManifest\\Tests\\": "tests/"
        }
    }
}
